{
    "name": "Best First Width Search (BFWS)",
    "description": "This package contains the source code of the planners that participated in IPC-9, 2018. It contains the winner of the Agile-Track, and runner-up of tha SAT-track.",
    "install-size": "0M",
    "dependencies": [
        "lapkt"
    ],
    "endpoint": {
        "services": {
            "solve": {
                "args": [
                    {
                        "name": "domain",
                        "type": "file",
                        "description": "domain file"
                    },
                    {
                        "name": "problem",
                        "type": "file",
                        "description": "problem file"
                    }
                ],
                "call": "dual-bfws-fdparser {domain} {problem} plan",
                "return": {
                    "type": "generic",
                    "files": "*plan*"
                }
            }
        }
    }
}