{
  "name": "ccxt.pro",
  "version": "1.1.10",
  "description": "A professional tool for algorithmic cryptocurrency trading",
  "main": "./ccxt.pro.js",
  "unpkg": "dist/ccxt.pro.browser.js",
  "engines": {
    "node": ">=7.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kroitor/ccxt.pro.git"
  },
  "readme": "README.md",
  "scripts": {
    "docker": "docker-compose run --rm ccxt",
    "build": "npm run pre-transpile && npm run transpile && npm run post-transpile",
    "force-build": "npm run pre-transpile && npm run force-transpile && npm run post-transpile",
    "pre-transpile": "npm run export-exchanges && npm run vss && npm run check-js-syntax && npm run browserify",
    "post-transpile": "npm run check-python-syntax && npm run check-php-syntax",
    "test": "npm run build && node run-tests",
    "fast-test": "node run-tests --js",
    "test-js": "node run-tests --js",
    "test-py": "node run-tests --python",
    "test-php": "node run-tests --php",
    "test-base": "npm run test-js-base && npm run test-python-base && npm run test-php-base",
    "cli.js": "node ./examples/js/cli.js",
    "cli.py": "python3 ./examples/py/cli.py",
    "cli.php": "php ./examples/php/cli.php",
    "test-js-base": "npm run test-js-cache && npm run test-js-orderbook",
    "test-js-cache": "node js/test/base/test.Cache.js",
    "test-js-orderbook": "node js/test/base/test.OrderBook.js",
    "test-python-base": "npm run test-python-cache && npm run test-python-orderbook",
    "test-python-cache": "python python/ccxtpro/test/test_cache.py",
    "test-python-orderbook": "python python/ccxtpro/test/test_order_book.py",
    "test-php-base": "npm run test-php-cache && npm run test-php-orderbook",
    "test-php-cache": "php -f php/test/Cache.php",
    "test-php-orderbook": "php -f php/test/OrderBook.php",
    "export-exchanges": "node build/export-exchanges",
    "transpile": "node build/transpile",
    "force-transpile": "node build/transpile --force",
    "vss": "node build/vss",
    "lint": "eslint",
    "check-syntax": "npm run transpile && npm run check-js-syntax && npm run check-python-syntax && npm run check-php-syntax",
    "check-js-syntax": "node -e \"console.log(process.cwd())\" && eslint --version && eslint \"js/*.js\" --cache --cache-location .cache/eslintcache --cache-strategy metadata",
    "check-python-syntax": "cd python && tox -e qa && cd ..",
    "check-php-syntax": "php -f php/test/syntax.php",
    "browserify": "browserify ./ccxt.pro.browser.js > ./dist/ccxt.pro.browser.js",
    "postinstall": "node postinstall"
  },
  "types": "./ccxt.pro.d.ts",
  "dependencies": {
    "ccxt": "^1.81.1",
    "ws": "^7.4.6"
  },
